<?xml version="1.0" encoding="UTF-8"?>
<CompileSettings>
    <ProjectSettings>
        <Content Scope="Included">
            <CompileGroup Type="BinderItem"/>
            <Filter State="Off" Exclude="No" Type="Label">
                <Label>-1</Label>
            </Filter>
        </Content>
        <Options>
            <ResampleImages DPI="72" MaxSize="0">No</ResampleImages>
            <ConvertRichTextToMMD>No</ConvertRichTextToMMD>
            <Links/>
            <PDF>
                <Cover Source="" Bleed="0.0" BleedUnits="pt"></Cover>
                <Compression Enabled="No">
                    <ImageResolutionCompressionType></ImageResolutionCompressionType>
                    <ImageResolution>300</ImageResolution>
                    <CompressionQuality>0.5</CompressionQuality>
                </Compression>
            </PDF>
            <Scriptwriting>
                <IncludeTitles>Yes</IncludeTitles>
                <IncludeSynopses>Yes</IncludeSynopses>
                <CommentsAsScriptNotes>Yes</CommentsAsScriptNotes>
            </Scriptwriting>
            <Ebook>
                <Cover>
                    <ImageDocument Source="FrontMatter"></ImageDocument>
                    <Title>Cover</Title>
                    <SVG Width="-1" Height="-1"><![CDATA[]]></SVG>
                </Cover>
                <TOC>
                    <HTML Generate="Yes">
                        <Title>Contents</Title>
                    </HTML>
                </TOC>
            </Ebook>
        </Options>
        <MetaData>
            <ProjectTitle>&lt;$template_projectName&gt;</ProjectTitle>
            <ProjectAbbreviatedTitle>&lt;$template_projectName&gt;</ProjectAbbreviatedTitle>
            <Surname>&lt;$template_lastName&gt;</Surname>
            <Forename>&lt;$template_firstName&gt;</Forename>
            <EbookLanguage>en</EbookLanguage>
            <FountainTitlePageMetaData>
                <Value Key="Title"><![CDATA[<$projecttitle>]]></Value>
                <Value Key="Author"><![CDATA[<$author>]]></Value>
            </FountainTitlePageMetaData>
            <MMDMetaData>
                <Value Key="Title"><![CDATA[<$projecttitle>]]></Value>
                <Value Key="Author"><![CDATA[<$author>]]></Value>
            </MMDMetaData>
        </MetaData>
    </ProjectSettings>
    <CurrentFileType>pdf</CurrentFileType>
    <FormatSettings>
        <Format ID="DEFAULT">
            <SectionLayouts>
                <Type ID="534B48ED-5D59-4081-8745-F913E42ABC50">NEW-PAGE-HEADER-WITH-TEXT</Type>
                <Type ID="96672DFA-5B1E-4158-8B41-9B437D334703">TEXT-SECTION</Type>
            </SectionLayouts>
        </Format>
    </FormatSettings>
    <LastUsedFormats>
        <Print>DEFAULT</Print>
        <pdf>DEFAULT</pdf>
    </LastUsedFormats>
</CompileSettings>
